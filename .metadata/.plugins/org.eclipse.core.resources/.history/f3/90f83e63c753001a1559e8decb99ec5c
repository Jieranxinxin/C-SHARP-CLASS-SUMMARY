package com;

public class 第三题 {
	public static void main(String[] args) {
		int[] months=new int[12];
		boolean[] members= {false,true,true,false};
		
		System.out.println("months的长度"+months.length);
		System.out.println("members的长度"+members.length);
		System.out.println("数组members的第三个值为"+members[12]);
	}

}
